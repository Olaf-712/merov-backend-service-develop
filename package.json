{
  "name": "merov-backend-service",
  "version": "0.1.0",
  "description": "Merov Backend Service",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prepare": "husky install",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "prettier": "prettier --write \"src/**/*.ts\" ",
    "prisma:generate": "npx prisma generate",
    "prisma:migrate": "npx prisma migrate dev",
    "ci": "yarn install --immutable --immutable-cache --check-cache",
    "deploy": "npm ci && npm run build && npm run start",
    "opensearch:create-indexes": "ts-node ./scripts/opensearch/create-indexes.ts",
    "nwrusa": "ts-node ./scripts/nwrusa.ts",
    "prove": "ts-node ./script s/prove.ts",
    "start": "pm2 startOrRestart ecosystem.config.js https://leetcode.com/problems/maximum-twin-sum-of-a-linked-list/&& pm2 logs",
    "start:dev": "npx nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "npx prisma migrate deploy && pm2 startOrRestart ecosystem.config.js && pm2 logs",
    "start:production": "npx prisma migrate deploy && pm2 startOrRestart ecosystem.config.js && pm2 logs",
    "start:staging": "npx prisma migrate deploy && pm2 startOrRestart ecosystem.config.js && pm2 logs",
    "start:development": "npx prisma migrate deploy && pm2 startOrRestart ecosystem.config.js && pm2 logs",
    "start:test": "npx prisma db push && pm2 startOrRestart ecosystem.config.js && pm2 logs",
    "stop": "pm2 stop all",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "sendbird": "ts-node ./scripts/sendbird.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@aws-sdk/client-ses": "^3.105.0",
    "@aws-sdk/client-sns": "^3.100.0",
    "@axiomhq/axiom-node": "^0.8.0",
    "@hapi/joi": "^17.1.1",
    "@nestjs/common": "^9.0.8",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.8",
    "@nestjs/event-emitter": "^1.3.1",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/mapped-types": "^1.1.0",
    "@nestjs/microservices": "^9.0.8",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^9.0.8",
    "@nestjs/schedule": "^2.1.0",
    "@nestjs/swagger": "^6.0.5",
    "@nestjs/throttler": "^3.0.0",
    "@ntegral/nestjs-sentry": "^4.0.0",
    "@prisma/client": "^3.15.2",
    "@sentry/node": "^7.17.3",
    "@sentry/tracing": "^7.17.3",
    "@slack/client": "^5.0.2",
    "@types/hapi__joi": "^17.1.8",
    "aws-sdk": "^2.1157.0",
    "axios": "^0.27.2",
    "cache-manager": "^4.0.1",
    "chalk": "4",
    "cheerio": "^1.0.0-rc.12",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "dotenv": "^16.0.1",
    "ethers": "^5.6.8",
    "firebase-admin": "^11.3.0",
    "helmet": "^5.1.0",
    "husky": "^8.0.1",
    "joi": "^17.6.0",
    "jwks-rsa": "^2.1.4",
    "moment": "^2.29.4",
    "multicoin-address-validator": "^0.5.10",
    "nanoid": "^3.0.0",
    "nest-winston": "^1.6.2",
    "passport": "^0.6.0",
    "passport-auth0": "^1.4.2",
    "passport-jwt": "^4.0.0",
    "pm2": "^5.2.0",
    "qs": "^6.11.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "twilio": "^3.84.0",
    "typescript-class-validator": "0.0.6",
    "winston": "^3.6.0",
    "yarn": "^1.22.19"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/async": "^3.2.18",
    "@types/cache-manager": "^4.0.0",
    "@types/cron": "^2.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "27.5.0",
    "@types/joi": "^17.2.3",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.0.0",
    "@types/passport-jwt": "^3.0.6",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "async": "^3.2.4",
    "csv-parse": "^5.3.5",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "28.0.3",
    "prettier": "^2.3.2",
    "prisma": "^3.15.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "28.0.1",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.0.0",
    "typescript": "^4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/src/"
    ]
  },
  "prisma": {
    "schema": "src/prisma/schema.prisma",
    "seed": "ts-node src/prisma/seed.ts"
  }
}
